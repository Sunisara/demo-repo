pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        echo 'This is a build step.'
      }
    }
    stage('test') {
      steps {
        echo 'This is a testing stage.'
      }
    }
    stage('deploy') {
      steps {
        echo 'This is a deploy stage.'
      }
    }
  }
}